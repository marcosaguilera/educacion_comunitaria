<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.aprendoz_desarrollodb.data.CostosAnuales" table="costos_anuales" catalog="aprendoz_desarrollo" dynamic-insert="false" dynamic-update="false">
        <id name="idCostoAnua" type="integer">
            <column name="id_costo_anua"/>
            <generator class="identity"/>
        </id>
        <property name="codigo" type="string">
            <column name="codigo" length="6" not-null="true"/>
        </property>
        <property name="nombreProducto" type="string">
            <column name="nombre_producto" length="200" not-null="true"/>
        </property>
        <property name="descripcion" type="string">
            <column name="descripcion" length="65535"/>
        </property>
        <property name="valor" type="double">
            <column name="valor" precision="22" not-null="true"/>
        </property>
        <property name="nuevo" type="boolean">
            <column name="nuevo" not-null="true"/>
        </property>
        <property name="obligatorio" type="boolean">
            <column name="obligatorio" not-null="true"/>
        </property>
        <property name="tipoCosto" type="integer">
            <column name="tipo_costo"/>
        </property>
        <many-to-one name="grado" class="com.aprendoz_desarrollodb.data.Grado">
            <column name="Grado_Id_Grado" not-null="true"/>
        </many-to-one>
        <many-to-one name="sy" class="com.aprendoz_desarrollodb.data.Sy">
            <column name="SY_Id_SY" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>